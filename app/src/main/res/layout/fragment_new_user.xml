        <android.support.v7.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/rv_response_employee">

        </android.support.v7.widget.RecyclerView>

    </LinearLayout>



</FrameLayout>